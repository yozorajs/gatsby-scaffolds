{
  "name": "@yozora/gatsby-images",
  "version": "2.1.0-alpha.7",
  "description": "Transform markdown files to Yozora AST",
  "author": {
    "name": "guanghechen",
    "url": "https://github.com/guanghechen/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yozorajs/gatsby-scaffolds.git",
    "directory": "packages/gatsby-images"
  },
  "homepage": "https://github.com/yozorajs/gatsby-scaffolds/tree/main/packages/gatsby-images#readme",
  "keywords": [
    "gatsby",
    "markdown",
    "gfm",
    "yozora",
    "images"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./lib/types/index.d.ts",
      "import": "./lib/esm/index.mjs"
    },
    "./gatsby-browser.js": {
      "import": "./gatsby-browser.js"
    },
    "./gatsby-node.js": {
      "import": "./gatsby-node.js"
    }
  },
  "module": "./lib/esm/index.mjs",
  "types": "./lib/types/index.d.ts",
  "source": "./src/index.ts",
  "license": "MIT",
  "engines": {
    "node": ">= 18.0.0"
  },
  "files": [
    "lib/",
    "gatsby-node.js",
    "gatsby-browser.js",
    "package.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "rimraf lib/ gatsby-browser.js gatsby-node.js && cross-env NODE_ENV=production rollup -c ./rollup.config.mjs",
    "prepublishOnly": "yarn build"
  },
  "dependencies": {
    "@yozora/gatsby-transformer": "^2.1.0-alpha.7",
    "chalk": "^5.2.0",
    "query-string": "^8.1.0"
  },
  "peerDependencies": {
    "@yozora/ast": "^2.0.0",
    "@yozora/ast-util": "^2.0.0",
    "gatsby": "^5.0.0",
    "gatsby-plugin-sharp": "^5.0.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "rimraf": "^3.0.2",
    "rollup": "^3.9.1"
  }
}

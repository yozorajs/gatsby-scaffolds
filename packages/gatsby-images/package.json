{
  "name": "@yozora/gatsby-images",
  "version": "2.1.0-alpha.1",
  "description": "Transform markdown files to Yozora AST",
  "author": {
    "name": "guanghechen",
    "url": "https://github.com/guanghechen/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yozorajs/gatsby-scaffolds.git",
    "directory": "packages/gatsby-images"
  },
  "homepage": "https://github.com/yozorajs/gatsby-scaffolds/tree/main/packages/gatsby-images#readme",
  "keywords": [
    "gatsby",
    "markdown",
    "gfm",
    "yozora",
    "images"
  ],
  "type": "module",
  "exports": "./lib/esm/index.js",
  "types": "lib/types/index.d.ts",
  "source": "src/index.ts",
  "license": "MIT",
  "engines": {
    "node": ">= 18.0.0"
  },
  "files": [
    "lib/",
    "!lib/**/*.js.map",
    "!lib/**/*.d.ts.map",
    "gatsby-node.js",
    "package.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development rollup -c -w ../../rollup.config.mjs",
    "build": "cross-env NODE_ENV=production rollup -c ../../rollup.config.mjs",
    "prebuild": "rimraf lib/",
    "prepublishOnly": "cross-env ROLLUP_SHOULD_SOURCEMAP=false yarn build"
  },
  "dependencies": {
    "@yozora/gatsby-transformer": "^2.1.0-alpha.1",
    "chalk": "^5.2.0",
    "gatsby-core-utils": "^4.3.1",
    "query-string": "^8.1.0"
  },
  "peerDependencies": {
    "@yozora/ast": "^2.0.0",
    "@yozora/ast-util": "^2.0.0",
    "gatsby": "^5.0.0",
    "gatsby-plugin-sharp": "^5.0.0"
  }
}

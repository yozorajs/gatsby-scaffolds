{
  "name": "@yozora/gatsby-transformer",
  "version": "1.0.0-alpha.15",
  "description": "Transform markdown files to Yozora AST",
  "author": {
    "name": "guanghechen",
    "url": "https://github.com/guanghechen/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yozorajs/gatsby-scaffolds.git",
    "directory": "packages/gatsby-transformer"
  },
  "homepage": "https://github.com/yozorajs/gatsby-scaffolds/tree/main/packages/gatsby-transformer#readme",
  "keywords": [
    "gatsby",
    "markdown",
    "gfm",
    "yozora"
  ],
  "main": "gatsby-node.js",
  "types": "lib/types/index.d.ts",
  "source": "src/index.ts",
  "license": "MIT",
  "engines": {
    "node": ">= 14.15.0"
  },
  "files": [
    "lib/",
    "!lib/**/*.js.map",
    "!lib/**/*.d.ts.map",
    "gatsby-node.js",
    "package.json",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=development rollup -c -w ../../rollup.config.js",
    "build": "cross-env NODE_ENV=production rollup -c ../../rollup.config.js",
    "prebuild": "rimraf lib/ gatsby-node.js && tsc -p tsconfig.src.json --emitDeclarationOnly",
    "prepublishOnly": "cross-env ROLLUP_SHOULD_SOURCEMAP=false yarn build"
  },
  "dependencies": {
    "@guanghechen/option-helper": "^1.8.2",
    "@guanghechen/parse-lineno": "^1.8.2",
    "@types/fs-extra": "^9.0.12",
    "dayjs": "^1.10.6",
    "fs-extra": "^10.0.0",
    "gray-matter": "^4.0.3"
  },
  "peerDependencies": {
    "@yozora/ast": "^1.2.0-alpha.0",
    "@yozora/ast-util": "^1.2.0-alpha.0",
    "@yozora/character": "^1.2.0-alpha.0",
    "@yozora/html-markdown": "^1.0.0-alpha.2",
    "gatsby": "^3.0.0-next.0"
  }
}
